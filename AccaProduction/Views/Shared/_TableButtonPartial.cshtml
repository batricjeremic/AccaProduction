@model AccaProduction.Models.IndividualButtonPartial

<td style="width:150px;">
    <div class="btn-group" role="group">
        @Html.Partial("_IndividualButtonPartial", new IndividualButtonPartial
        {
            Action = "Edit",
                    ButtonType = "btn-primary",
                    Glyph = "pencil",
                    Text = "Edit",
                    KandidatId = Model.KandidatId,
                    RokId = Model.RokId
        })
        @Html.Partial("_IndividualButtonPartial", new IndividualButtonPartial
       {
           Action = "Details",
           ButtonType = "btn-success",
           Glyph = "list",
           Text = "Details",
           KandidatId = Model.KandidatId,
           RokId = Model.RokId

       })
        @Html.Partial("_IndividualButtonPartial", new IndividualButtonPartial
   {
       Action = "Delete",
       ButtonType = "btn-danger",
       Glyph = "trash",
       Text = "Edit",
       KandidatId = Model.KandidatId,
       RokId = Model.RokId
   })
    </div>
</td>